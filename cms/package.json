    "@payloadcms/richtext-lexical": "3.13.0",
    "@payloadcms/storage-s3": "3.13.0",
    "@payloadcms/translations": "^3.13.0",
    "@payloadcms/ui": "^3.13.0",
    "bson": "^6.10.1",
    "cookie": "^1.0.2",
    "cross-env": "^7.0.3",
    "deepl-node": "^1.15.0",
    "jsdom": "^25.0.1",
    "lexical": "0.20.0",
