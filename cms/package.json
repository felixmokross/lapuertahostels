{
  "name": "cms",
  "version": "0.0.0-unknown",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "npm run generate:types && npm run generate:importmap && cross-env NODE_OPTIONS=--no-deprecation next build",
    "prod-preview": "npm run build && mv .next/static .next/standalone/.next/static && cross-env PORT=3001 node .next/standalone/server.js",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation PORT=3001 next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "touch \"src/app/(payload)/admin/importMap.js\" && cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation PORT=3001 next start",
    "check-format": "prettier --check ."
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "^3.5.0",
    "@payloadcms/next": "^3.5.0",
    "@payloadcms/richtext-lexical": "^3.5.0",
    "@payloadcms/storage-s3": "^3.5.0",
    "cookie": "^1.0.2",
    "cross-env": "^7.0.3",
    "deepl-node": "^1.15.0",
    "jsdom": "^25.0.1",
    "next": "15.0.4",
    "openai": "^4.76.0",
    "payload": "^3.5.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "0.32.6"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.5.4",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8.4.49",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.15",
    "typescript": "5.6.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
