{
  "name": "@lapuertahostels/cms",
  "private": true,
  "version": "0.0.0-unknown",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "pnpm generate:types && pnpm generate:importmap && cross-env NODE_OPTIONS=--no-deprecation next build",
    "prod-preview": "pnpm build && mv .next/static .next/standalone/.next/static && cross-env PORT=3001 node .next/standalone/server.js",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation PORT=3001 next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "touch \"src/app/(payload)/admin/importMap.js\" && cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation PORT=3001 next start",
    "check-format": "prettier --check ."
  },
  "dependencies": {
    "@lexical/headless": "0.20.0",
    "@lexical/html": "0.20.0",
    "@payloadcms/db-mongodb": "^3.15.1",
    "@payloadcms/next": "^3.15.1",
    "@payloadcms/richtext-lexical": "^3.15.1",
    "@payloadcms/storage-s3": "^3.15.1",
    "@payloadcms/translations": "^3.15.1",
    "@payloadcms/ui": "^3.15.1",
    "bson": "^6.10.1",
    "cookie": "^1.0.2",
    "cross-env": "^7.0.3",
    "deepl-node": "^1.15.0",
    "jsdom": "^25.0.1",
    "lexical": "0.20.0",
    "next": "^15.1.4",
    "openai": "^4.77.4",
    "payload": "^3.15.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "0.32.6"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.1.4",
    "postcss": "^8.4.49",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "typescript": "5.6.3"
